{% extends "base.twig" %}

{% block content %}

	hello
	<main class="timeline">

		<div class="container">
			{{ post.get_field('headline') }}


			{% for post in history %}
				<a href="{{ post.link }}">
					<h2>{{post.year}}</h2>
					<h2>{{ post.title }}</h2>
				</a>

				<p>{{post.description}}</p>
				<img src="{{ Image(post.meta('image')).src }}" alt="">


			{% endfor %}
		</div>


	</main>
{% endblock %}
