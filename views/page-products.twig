{% extends "base.twig" %}

{% block content %}
	<h1>{{post.headline}}</h1>
	{{post.meta('content')}}
	<ul class="products__list">
		{% for product in products %}
			<li class="products__category">
				<a href="{{ product.link }}">{{ product.name }}</a>
			</li>
		{% endfor %}
	</ul>
	{% include "templates/contact.twig" %}

{% endblock %}
